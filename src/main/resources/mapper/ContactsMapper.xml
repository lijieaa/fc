  <!--查询客户下人员-->
  <select id="byiId" resultType="com.jianpanmao.customer.dto.CustomerContactDto" parameterType="java.lang.Integer">
    SELECT c.con_id id,c.con_name name,c.cus_id cusId
    FROM contacts c LEFT JOIN customer cu ON c.cus_id = cu.cus_id
    WHERE cu.intermediary_id = #{0}
  </select>


