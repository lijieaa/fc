  <select id="deviceContacts" resultMap="DeviceResultMap">
select
c.*,dc.device_contacts_status
from contacts c LEFT JOIN device_contacts dc ON c.con_id = dc.device_contacts_cid
LEFT JOIN device d ON dc.device_contacts_did = d.device_id
WHERE d.device_id = #{0}
